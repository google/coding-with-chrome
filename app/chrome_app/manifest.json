{
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },
  "name": "Coding with Chrome",
  "short_name": "Coding with Chrome",
  "description": "Learn, improve, or teach coding skills within a Chrome browser.",
  "key": "",
  "version": "5.10.1",
  "oauth2": {
    "client_id": "418162477970-2qmsb2sp3m522hf292154favsjgpsj9n.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive",
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/cloud-platform",
      "https://www.googleapis.com/auth/cloud-platform.read-only",
      "https://www.googleapis.com/auth/devstorage.full_control",
      "https://www.googleapis.com/auth/devstorage.read_only",
      "https://www.googleapis.com/auth/devstorage.read_write",
      "https://www.googleapis.com/auth/classroom.courses.readonly",
      "https://www.googleapis.com/auth/classroom.coursework.me.readonly"
    ]
  },
  "icons": {
    "16":  "icons/16x16.png",
    "32":  "icons/32x32.png",
    "64":  "icons/64x64.png",
    "128": "icons/128x128.png",
    "256": "icons/256x256.png"
  },
  "author": "Markus Bordihn (mbordihn@google.com)",
  "file_handlers": {
    "supported": {
      "extensions": [
        "cwc", "txt", "html", "htm", "js", "coffee", "py"
      ]
    },
    "text": {
      "extensions": [
        "text/*"
      ]
    }
  },
  "sandbox": {
    "pages": [
      "LICENSE.html",
      "NOTICE.html"
    ]
  },
  "webview": {
    "partitions": [{
        "name": "static",
        "accessible_resources": [
          "LICENSE.html",
          "NOTICE.html"
        ]
      }
    ]
  },
  "minimum_chrome_version": "42",
  "offline_enabled": true,
  "bluetooth": {
    "uuids": [
      "00001101-0000-1000-8000-00805f9b34fb",
      "00000000-deca-fade-deca-deafdecacaff"
    ],
    "socket": true
  },
  "permissions": [
    "alwaysOnTopWindows",
    "browser",
    "clipboardRead",
    "clipboardWrite",
    "contextMenus",
    "fullscreen",
    "storage", {
      "fileSystem": ["write"]
    },
    "identity",
    "mdns",
    "serial",
    "system.network",
    "tts",
    "unlimitedStorage",
    "usb",
    "webview"
  ],
  "sockets": {
    "tcpServer" : {
      "listen": ["*"]
    }
  }
}
