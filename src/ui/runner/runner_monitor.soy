{namespace cwc.soy.ui.RunnerMonitor autoescape="strict"}


/**
 * Monitor template.
 */
{template .template}
  {@param prefix: string}

  <div class="mdl-layout mdl-js-layout mdl-layout--fixed-header mdl-layout--fixed-tabs">
    <header id="{$prefix}toolbar" class="mdl-layout__header bg_10_pct_black">
      <div class="mdl-layout__tab-bar mdl-js-ripple-effect">
        <a href="#{$prefix}main" class="mdl-layout__tab is-active">
          <span class="icon_24px icon_button">send</span>
        </a>
        <a href="#{$prefix}intro" id="{$prefix}tab-intro" class="mdl-layout__tab">
          <span class="icon_24px icon_button">developer_board</span>
        </a>
        <a href="#{$prefix}monitor" id="{$prefix}tab-monitor" class="mdl-layout__tab">
          <span class="icon_24px icon_button">equalizer</span>
        </a>
        <a href="#{$prefix}control" id="{$prefix}tab-control" class="mdl-layout__tab">
          <span class="icon_24px icon_button">gamepad</span>
        </a>
        <a href="#{$prefix}calibration" id="{$prefix}tab-calibration" class="mdl-layout__tab">
          <span class="icon_24px icon_button">tune</span>
        </a>
      </div>
    </header>
    <div id="{$prefix}content" class="mdl-layout__content">
      <section id="{$prefix}main" class="mdl-layout__tab-panel is-active">
        <div id="{$prefix}main-run" class="icon_auto icon_button is-active">play_arrow</div>
        <div class="mdl-tooltip mdl-tooltip--top" for="{$prefix}main-run">
          {msg desc=""}
            Executes the code and send commands to the connected unit
          {/msg}
        </div>
        <div id="{$prefix}main-stop" class="icon_auto icon_button">stop</div>
        <div class="mdl-tooltip mdl-tooltip--top" for="{$prefix}main-stop">
          {msg desc=""}
            Stops the execution of the code
          {/msg}
        </div>
      </section>
      <section id="{$prefix}intro" class="mdl-layout__tab-panel">
        {msg desc=""}Intro{/msg}
      </section>
      <section id="{$prefix}monitor" class="mdl-layout__tab-panel">
        {msg desc=""}Monitor{/msg}
      </section>
      <section id="{$prefix}control" class="mdl-layout__tab-panel">
        {msg desc=""}Control{/msg}
      </section>
      <section id="{$prefix}calibration" class="mdl-layout__tab-panel">
        {msg desc=""}Calibration{/msg}
      </section>
    </div>
  </div>
  <div id="{$prefix}statusbar"></div>
{/template}
