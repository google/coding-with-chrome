{namespace cwc.soy.SelectScreenAdvanced autoescape="strict"}


/**
 * Advanced overview page.
 */
{template .overview}
  {@param prefix: string}
  <div id="{$prefix}overview">

    {call .overviewHeader_}
      {param prefix: $prefix /}
      {param active_link: 'home' /}
    {/call}

    {call .header_}
      {param title: 'Advanced Coding' /}
      {param opt_color_class: 'bg-light-blue' /}
      {param opt_icon: 'subject' /}
    {/call}

    <div class="mdl-grid">

      {call .selectCard_}
        {param prefix: $prefix /}
        {param title: 'Simple' /}
        {param text: 'JavaScript made easy through easy starter functions' /}
        {param link_text: 'Go Simple' /}
        {param link_id: 'basic' /}
        {param opt_color_class: 'bg-light-blue' /}
        {param opt_icon: 'school' /}
      {/call}

      {call .selectCard_}
        {param prefix: $prefix /}
        {param title: 'Programming' /}
        {param text: 'Build software with text based programming languages' /}
        {param link_text: 'Go Coding ' /}
        {param link_id: 'programming-language' /}
        {param opt_color_class: 'bg-blue' /}
        {param opt_icon: 'view_stream' /}
      {/call}

      {call .selectCard_}
        {param prefix: $prefix /}
        {param title: 'Markup' /}
        {param text: 'Put together the components of a website- HTML, CSS and JavaScript' /}
        {param link_text: 'Go Web' /}
        {param link_id: 'markup-language' /}
        {param opt_color_class: 'bg-blue' /}
        {param opt_icon: 'public' /}
      {/call}

      {call .selectCard_}
        {param prefix: $prefix /}
        {param title: 'Robots' /}
        {param text: 'Control virtual or real robots' /}
        {param link_text: 'Go Robots' /}
        {param link_id: 'robot' /}
        {param opt_color_class: 'bg-orange' /}
        {param opt_icon: 'memory' /}
      {/call}

      {call .selectCard_}
        {param prefix: $prefix /}
        {param title: '3D' /}
        {param text: 'Create create and display animated 3D computer graphics' /}
        {param link_text: 'Start Drawing' /}
        {param link_id: '3d' /}
        {param opt_color_class: 'bg-blue' /}
        {param opt_icon: '3d_rotation' /}
      {/call}

    </div>
  </div>
{/template}


