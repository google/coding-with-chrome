/**
 * Header design.
 */
{template .header_}
  {@param title: string}
  {@param? opt_icon: string}
  {@param? opt_text: string}
  {@param? opt_color_class: string}

  <header class="cwc-band{if $opt_color_class} {$opt_color_class}{/if}">
    {if $opt_icon}<div class="cwc-band-icon icon_auto">{$opt_icon}</div>{/if}
    <div class="cwc-band-text">
      <div class="mdl-typography--display-2 mdl-typography--font-thin">{$title}</div>
      {if $opt_text}<p class="mdl-typography--headline mdl-typography--font-thin">{$opt_text}</p>{/if}
    </div>
  </header>
{/template}


/**
 * Overview header.
 */
{template .overviewHeader_}
  {@param prefix: string}
  {@param active_link: string}

  <header id="{$prefix}navigation-overview" class="cwc-navigation mdl-layout__header-row mdl-layout__header bg_10_pct_black">
    <nav class="mdl-navigation mdl-typography--body-1-force-preferred-font">
      <a id="{$prefix}menu-home" class="mdl-navigation__link icon_24px_raw{if $active_link == 'home'} is-active{/if}" href="#">
        home</a>
