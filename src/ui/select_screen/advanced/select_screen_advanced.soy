      {$text}{if $opt_text}<br>{$opt_text}{/if}
    </div>
  </footer>
{/template}


/**
 * CSS style definitions.
 */
{template .style}
  {@param prefix: string}

  .{$prefix}overview section.section--center,
  .{$prefix}overview .max-width {lb}
    max-width: 860px;
    margin: 20px auto;
  {rb}

  #{$prefix}overview .mdl-card__media {lb}
    min-height: 150px;
    overflow: hidden;
  {rb}

  #{$prefix}overview .mdl-card__media img {lb}
    width: 100%;
  {rb}

  #{$prefix}overview .mdl-card__title {lb}
    background: transparent;
    height: auto;
  {rb}

  #{$prefix}overview .mdl-card__title-text {lb}
    color: black;
    height: auto;
  {rb}

  #{$prefix}overview .mdl-card__supporting-text {lb}
    height: auto;
    color: black;
    padding-bottom: 56px;
  {rb}

  #{$prefix}overview .mdl-card__actions {lb}
    position: absolute;
    bottom: 0;
  {rb}

  #{$prefix}overview .mdl-card__actions a {lb}
    border-top: none;
    font-size: 16px;
    color: #8bc34a;
  {rb}

{/template}
