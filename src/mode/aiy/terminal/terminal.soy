{namespace cwc.soy.mode.aiy.Terminal autoescape="strict"}


/**
 * Terminal template.
 */
{template .template}
  {@param prefix: string}

  <div id="{$prefix}body">
    <div id="{$prefix}content"></div>
    <div id="{$prefix}toolbar">
      <button id="{$prefix}clear" class="mdl-button mdl-js-button mdl-button--icon"
        title="{msg desc=""}@@CONSOLE__CLEAR{/msg}">
        <i class="material-icons">not_interested</i>
      </button>
    </div>
  </div>
{/template}
