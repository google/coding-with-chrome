#{$prefix}aiy-container {
  flex-direction: column;
  display: flex;
  overflow: hidden;
  height: 100%;
}

#{$prefix}aiy-toolbar {
  display: flex !important;
  min-height: 34px;
  padding-bottom: 15px;
}

#{$prefix}aiy-toolbar > .mdl-layout__header-row {
  height: 34px;
}

#{$prefix}aiy-toolbar > .mdl-layout__header-row .mdl-navigation__link {
  padding: 0 5px;
}
